{
	"schemaVersion": 1,
	"id": "fastitems",
	"version": "${version}",
	"name": "Fast Items",
	"description": "Optimize item rendering by transforming item entities to 2D",
	"icon": "assets/fastitems/icon.png",
	"authors": [
		"Noryea"
	],
	"contact": {
		"sources": "https://github.com/Noryea/fast-items-fabric",
		"issues": "https://github.com/Noryea/fast-items-fabric/issues"
	},
	"license": "CC0-1.0",
	"environment": "client",
	"entrypoints": {
		"client": [
          "cn.noryea.fastitems.FastItemsClient"
		]
	},
	"mixins": [
		"fastitems.mixins.json"
	],
	"breaks": {
		"minecraft": ">=1.20.5"
	},
	"depends": {
		"fabricloader": ">=0.14.21",
		"midnightlib": "*",
		"minecraft": ">=1.20.3",
		"java": ">=17"
	}
}